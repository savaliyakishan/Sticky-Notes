{
  "manifest_version": 3,
  "name": "Mini Sticky Notes KS",
  "version": "1.3.2",
  "description": "Quickly take a note of anything, anytime.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  },
  "host_permissions": ["<all_urls>"]
}
